      "post": {
        "tags": ["User"],
        "summary": "Create a new user",
        "description": "This endpoint allows create a new user and expects a JSON object in the request body containing the details of the user to be created. The JSON object should include required fields such as name, email, password and role.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "items": {
                  "type": "object"
                },
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "email": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  },
                  "confirmPassword": {
                    "type": "string"
                  },
                  "role": {
                    "type": "number"
                  }
                },
                "example": {
                  "name": "John Doe",
                  "email": "johndoe@test.com",
                  "password": "123456",
                  "confirmPassword": "123456",
                  "role": 2
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created"
          },
          "500": {
            "description": "User already exists"
          }
        }
      },
